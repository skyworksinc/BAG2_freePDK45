#! /usr/local/bin/tcsh -f

# set BAG working directory
setenv BAG_WORK_DIR `pwd`
# set BAG framework directory
setenv BAG_FRAMEWORK "${BAG_WORK_DIR}/BAG_framework"
# set BAG python executable
# setenv BAG_PYTHON "/tools/projects/eeis/BAG_2.0/anaconda2/bin/python"
# uncomment to use python 3
setenv BAG_PYTHON "/tools/projects/eeis/BAG_2.0/anaconda3/bin/python"
#setenv BAG_PYTHON "/tools/projects/erichang/programs/anaconda3/bin/python"
# set technology/project configuration directory
setenv BAG_TECH_CONFIG_DIR "${BAG_WORK_DIR}/freePDK45"
# set where BAG saves temporary files
setenv BAG_TEMP_DIR "/tools/scratch/${USER}/BAGTMP"
# set location of BAG configuration file
setenv BAG_CONFIG_PATH "${BAG_WORK_DIR}/bag_config.yaml"

# set IPython configuration directory
setenv IPYTHONDIR "$BAG_WORK_DIR/.ipython"
# set IPython configuration directory
setenv JUPYTER_CONFIG_DIR "$BAG_WORK_DIR/.jupyter"
